{
  "name": "my-electron-app",
  "version": "1.0.0",
  "description": "An example Electron app",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "dist": "npm run build",
    "test": "jest"
  },
  "devDependencies": {
    "electron": "^25.2.0",
    "electron-builder": "^24.6.0",
    "jest": "^29.7.0"
  },
  "build": {
    "appId": "com.example.myapp",
    "productName": "MyElectronApp",
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "main.js",
      "package.json"
    ],
    "directories": {
      "output": "release"
    }
  }
}
